<body>

<title>Exam 1 CBT version |</title>


<div class="header">
  <div id="header-wrapper">
  <table border="0" width="100%" cellpadding="0" cellspacing="0">
    <tr>
      <td width="30%" class="header-cell" align="left" >
        Question <span class="question_counter">0</span> of 30      </td>
      
      <td class="header-cell" width="30%" align="right" >
        Time Left: 
        <span id="timer">
        00:00:00
        </span>
      </td>
    </tr>
    <tr>
      <td align="left" colspan="2">
        <span class="review-flag" style="display: none;">
          Reviewing 0  questions        </span>
      </td>
            <td align="right">
        <span style="cursor: pointer;" onclick="openExhibit(); return false;"><img 
          alt="exhibit" border="0" hspace="0" vspace="0" 
           /></span>
        <span style="cursor: pointer; display: none; " 
          onclick="openCalc(); return false;"><img 
          alt="CDF calculator" border="0" hspace="0" vspace="0" 
          /></span>
      </td>
    </tr>
  </table>

  </div>
</div>


<div id="content" style="display: none;">
 
<div class="question"> 
  <div id='questionimage'>
   <%= image_tag'https://s3-us-west-1.amazonaws.com/soaquestions/soasample.png'%>
 </div>
  <img alt="Question 0"/>
 

<br/>


<input type="hidden" id="idx_count" name="idx_count" value="127" />

</div>
 

 <div id="circle4" class="circle"></div>

</div>


<div class="footer">
  <div class="footer-content" align="center">
        <div class="footer-btn">
      <span style="cursor: pointer;" onclick="previous();"><img 
        alt="previous" border="0" hspace="0" vspace="0" 
         /><%= link_to 'previous', exam_question_path({"exam_id"=>"1",
 "id"=>"1"}) %></span>
    </div>
    <div class="footer-btn">
      <span onclick="next();" style="cursor: pointer;"><img 
        alt="next" border="0" hspace="0" vspace="0" 
        /></span>
    </div>
    <div class="footer-btn">
      <span onclick="mark();" style="cursor: pointer;"><img 
        alt="mark" border="0" hspace="0" vspace="0" 
        /></span>
    </div>
    <div class="footer-btn">
      <span style="cursor: pointer;" onclick="review();"><img 
        alt="review" border="0" hspace="0" vspace="0" 
      </span>
    </div>
        
  </div>
</div>
</body>
